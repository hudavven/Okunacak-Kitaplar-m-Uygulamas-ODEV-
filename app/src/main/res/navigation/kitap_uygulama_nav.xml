    <fragment
        android:id="@+id/fragmentProfil"
        android:name="com.example.mobil_programlama_odev.Activityler.FragmentProfil"
        android:label="fragment_profil"
        tools:layout="@layout/fragment_profil" />
    <fragment
        android:id="@+id/fragmentKitaplar"
        android:name="com.example.mobil_programlama_odev.Activityler.FragmentKitaplar"
        android:label="fragment_kitaplar"
        tools:layout="@layout/fragment_kitaplar" />
