<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/kitap_uygulama_nav"
    app:startDestination="@id/fragmentKatalog">

    <fragment
        android:id="@+id/fragmentKatalog"
        android:name="com.example.mobil_programlama_odev.Activityler.FragmentKatalog"
        android:label="fragment_katalog"
        tools:layout="@layout/fragment_katalog" >
